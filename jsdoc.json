{
    "source": {
      "include": ["./src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown","better-docs/component"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true,
      "better-docs": {
        "name": "Football Player Info App"
      }
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "template": "./node_modules/better-docs",
      "readme":"./README.md"
    }
  }